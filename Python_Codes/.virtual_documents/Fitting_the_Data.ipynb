from scipy.stats import norm
from matplotlib import colors

# Load data
x = np.loadtxt('./Two_Gaussian_Data.dat', usecols=(0), skiprows=1)
y = np.loadtxt('./Two_Gaussian_Data.dat', usecols=(1), skiprows=1)

